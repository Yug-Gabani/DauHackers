{
  "name": "coastalguard-pro-backend",
  "version": "2.0.0",
  "description": "Professional Coastal Threat Alert System with AI/ML, Government Data Integration, and Real-time Monitoring",
  "main": "backend/server.js",
  "scripts": {
    "start": "node backend/server.js",
    "dev": "nodemon backend/server.js",
    "prod": "NODE_ENV=production node backend/server.js",
    "prod:cluster": "NODE_ENV=production node backend/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "migrate": "node scripts/migrate.js",
    "health": "curl http://localhost:5000/health",
    "monitor": "node scripts/monitor.js",
    "benchmark": "node scripts/benchmark.js",
    "install:ai": "npm install @tensorflow/tfjs-node natural",
    "install:gov": "npm install axios node-cron",
    "install:all": "npm install && npm run install:ai && npm run install:gov"
  },
  "keywords": [
    "coastal",
    "threat",
    "alert",
    "ai",
    "ml",
    "machine-learning",
    "government",
    "api",
    "real-time",
    "monitoring",
    "disaster-management",
    "environmental-protection"
  ],
  "author": "CoastalGuard Professional Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.0",
    "twilio": "^4.19.0",
    "node-cron": "^3.0.3",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "nodemailer": "^6.9.7",
    "morgan": "^1.10.0",
    "hpp": "^0.2.3",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "@tensorflow/tfjs-node": "^4.15.0",
    "natural": "^6.10.4",
    "cluster": "^0.7.7",
    "os": "^0.1.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coastalguard/pro-backend.git"
  },
  "bugs": {
    "url": "https://github.com/coastalguard/pro-backend/issues"
  },
  "homepage": "https://coastalguard.pro",
  "funding": {
    "type": "patreon",
    "url": "https://patreon.com/coastalguard"
  }
}
